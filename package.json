{
  "name": "@gerhobbelt/jsdoc",
  "version": "4.0.0-3",
  "description": "An API documentation generator for JavaScript.",
  "keywords": [
    "documentation",
    "javascript"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsdoc2md/jsdoc"
  },
  "dependencies": {
    "@babel/parser": "7.4.5",
    "async": "3.0.1",
    "bluebird": "3.5.5",
    "catharsis": "0.8.10",
    "code-prettify": "0.1.0",
    "color-themes-for-google-code-prettify": "2.0.4",
    "escape-string-regexp": "2.0.0",
    "lodash": "4.17.21",
    "markdown-it": "10.0.0",
    "markdown-it-anchor": "5.2.1",
    "marked": "1.1.1",
    "mkdirp": "0.5.2",
    "open-sans-fonts": "1.5.1",
    "requizzle": "0.2.2",
    "strip-json-comments": "3.0.1",
    "taffydb": "2.6.2",
    "snyk": "^1.473.0"
  },
  "devDependencies": {
    "ajv": "6.10.0",
    "gulp": "4.0.2",
    "gulp-eslint": "5.0.0",
    "gulp-json-editor": "2.5.2",
    "jasmine": "3.4.0",
    "jasmine-console-reporter": "3.1.0",
    "klaw-sync": "6.0.0",
    "nyc": "14.1.1"
  },
  "engines": {
    "node": ">=8.15.0"
  },
  "scripts": {
    "build": "gulp lint; gulp test",
    "gulp-lint": "gulp lint",
    "lint": "eslint *.js lib/**/*.js plugins/*.js templates/**/*.js test/specs/**/*.js",
    "fix": "eslint *.js lib/**/*.js plugins/*.js templates/**/*.js test/specs/**/*.js --fix",
    "test": "node jsdoc.js -T",
    "pub": "npm publish --access public",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "bin": {
    "jsdoc": "./jsdoc.js"
  },
  "greenkeeper": {
    "ignore": [
      "taffydb"
    ]
  },
  "bugs": "https://github.com/jsdoc/jsdoc/issues",
  "author": {
    "name": "Michael Mathews",
    "email": "micmath@gmail.com"
  },
  "contributors": [
    {
      "url": "https://github.com/jsdoc/jsdoc/graphs/contributors"
    }
  ],
  "maintainers": [
    {
      "name": "Jeff Williams",
      "email": "jeffrey.l.williams@gmail.com"
    }
  ],
  "snyk": true
}
