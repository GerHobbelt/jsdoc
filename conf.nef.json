{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
	"includePattern": "^.+(atomicWorkers|compoundWorkers|lib/nef|nodeApps|wengui/webApps).+\\.js$",        
	"excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/nef.js"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "jsVersion": 180,
    "opts" : {
        "lenient" : "true"
    }
}
